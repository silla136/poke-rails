<div class="container">
  <div class="header">
    <%= link_to "< Back to trainers", trainers_path %>
    <h1>Sign up to train</h1>
  </div>
  <%= simple_form_for @trainer, html: { multipart: true } do |f| %>
    <%= f.input :name %>
    <%= f.input :age %>
    <%# <%= f.input :photo, as: :file %>
    <%= f.input :photo %>
    <%= f.button :submit, class: "btn poke-btn" %>
  <% end %>
</div>
